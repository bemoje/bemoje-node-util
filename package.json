{
  "name": "@bemoje/node-util",
  "version": "0.4.4",
  "description": "Various utilities for development in node-js",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "module": "dist/index.esm.js",
  "scripts": {
    "lint": "eslint \"*/**/*.{ts,js,json}\" --fix",
    "ts": "ts-node scripts/runTsFile.ts",
    "bundle": "ts-node scripts/bundle.ts",
    "entrypoint": "ts-node scripts/generateEntryPoint.ts",
    "teststats": "ts-node scripts/generateUnitTestStats.ts",
    "gendocs": "ts-node scripts/generateTsDocs.ts",
    "gentests": "ts-node scripts/generateUnitTests.ts",
    "gendocstests": "scripts\\gendocstests.bat",
    "test": "jest --preset ts-jest --coverage",
    "rollup": "rollup --config ./rollup.config.js --bundleConfigAsCjs",
    "build": "npm run entrypoint && npm run lint && rimraf dist && rimraf types && npm run rollup",
    "docs": "rimraf docs && typedoc --out docs src/index.ts --readme none --plugin typedoc-plugin-markdown --theme markdown --entryDocument index.md --publicPath /docs/",
    "prepublish": "npm run build && npm run test && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bemoje/bemoje-node-util.git"
