    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^8.37.0",
    "@types/esprima": "^4.0.3",
    "@types/exceljs": "^1.3.0",
    "@types/jest": "^26.0.24",
    "@types/level": "^6.0.1",
    "@types/lodash": "^4.14.194",
    "@types/mkdirp": "^2.0.0",
    "@types/node": "^18.11.18",
    "@types/object-hash": "^3.0.2",
    "@types/pdf-parse": "^1.1.1",
    "@types/prettier": "^2.7.2",
    "@types/title-case": "^1.1.2",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^26.6.3",
    "openai": "^3.3.0",
    "prettier": "^2.8.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.27.3",
    "ts-jest": "^26.5.6",
    "typedoc": "0.23.24",
    "typedoc-plugin-markdown": "3.14.0",
    "typescript": "^4.2.4",
    "walkdir": "^0.4.1"
