    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^26.6.3",
    "openai": "^3.2.1",
    "prettier": "^2.8.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.27.3",
    "ts-jest": "^26.5.6",
    "typedoc": "0.23.24",
    "typedoc-plugin-markdown": "3.14.0",
    "typescript": "^4.2.4",
    "walkdir": "^0.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bemoje/bemoje-node-util.git"
