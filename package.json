{
  "name": "@bemoje/node-util",
  "version": "0.6.1",
  "description": "Various utilities for development in node-js",
  "main": "dist/index.cjs.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/index.esm.js",
  "scripts": {
    "lint": "eslint \"*/**/*.{ts,js,json}\" --fix",
    "ts": "ts-node scripts/runTsFile.ts",
    "bundle": "ts-node scripts/bundle.ts",
    "entrypoint": "ts-node scripts/generateEntryPoint.ts",
    "teststats": "ts-node scripts/generateUnitTestStats.ts",
    "gendocs": "ts-node scripts/generateTsDocs.ts",
    "gentests": "npm run rollup && ts-node scripts/generateUnitTests.ts",
    "gendocstests": "scripts\\gendocstests.bat",
    "test": "jest --preset ts-jest --coverage",
    "rollup": "npm run entrypoint && rollup --config ./rollup.config.js --bundleConfigAsCjs",
    "build": "npm run lint && rimraf dist && npm run rollup",
    "docsmd": "rimraf docs/md && typedoc --out docs/md src/index.ts --readme none --plugin typedoc-plugin-markdown --theme markdown --entryDocument index.md --publicPath /docs/md/ && typedoc --out docs/html --entryPoints src/index.ts",
    "docshtml": "rimraf docs/html && typedoc --out docs/html --entryPoints src/index.ts",
    "docs": "rimraf docs && npm run docsmd && npm run docshtml",
    "viewdocs": "start C:\\Users\\bemoj\\repos\\bemoje-node-util\\docs\\html\\index.html",
    "prepub": "npm run build && npm run test && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bemoje/bemoje-node-util.git"
  },
  "author": {
    "name": "Benjamin MÃ¸ller Jensen",
    "email": "bemoje@bemoje.net",
    "url": "https://github.com/bemoje/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bemoje/bemoje-node-util/issues"
  },
  "homepage": "https://github.com/bemoje/bemoje-node-util",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/user?u=40752770"
  },
  "keywords": [
    "library",
    "typescript",
    "nodejs",
    "node",
    "util",
    "utilities",
    "tools",
    "development"
  ],
  "dependencies": {
    "deep-assign": "^3.0.0",
    "esprima": "^4.0.1npm ",
    "mkdirp": "^3.0.1",
    "strip-comments": "^2.0.1",
    "walkdir": "^0.4.1"
  },
  "devDependencies": {
    "@bemoje/trie-map": "^1.2.0",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/deep-assign": "^0.1.1",
    "@types/eslint": "^8.44.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/esprima": "^4.0.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.3",
    "@types/prettier": "^2.7.3",
    "@types/rollup": "^0.54.0",
    "@types/strip-comments": "^2.0.1",
    "@types/supertest": "^2.0.12",
    "@types/timsort": "^0.3.0",
    "@types/typescript": "^2.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "appdata-path": "^1.0.0",
    "cli-color": "^2.0.3",
    "concat-md": "^0.5.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "expiry-map": "^2.0.0",
    "extract-zip": "^2.0.1",
    "jest": "^29.6.1",
    "jschardet": "^3.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.1.6"
  }
}
